package streams;

import java.util.*;

public class ForEach {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();
		list.add("srikanth");
		list.add("abhiram");
		list.add("srikrishna");
		list.add("stevjobs");
		
		//normal way 
		 for (String string : list) {
			System.out.println(string);
		}
		
		//forEach method streams
		 
		 list.stream().forEach(t-> System.out.println(t));
		
		
		
	}

}
