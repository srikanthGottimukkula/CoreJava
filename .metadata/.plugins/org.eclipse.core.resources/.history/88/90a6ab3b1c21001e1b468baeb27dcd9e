package streams;
import java.util.*;

public class Filters {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		list.add("srikanth");
		list.add("abhiram");
		list.add("srikrishna");
		list.add("stevjobs");
		
		 for (String st : list) {
			if(st.startsWith("sr")) {
				System.out.println(st);
			}
		}
	}

}
